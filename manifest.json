{
	  "name": "Movie Tracker Extension",
	  "version": "0.9",
	  "description": "Extension to track new movies and put them on your calendar",
	"background_page": "background.html",
	"icons": { "128": "movies.png" },
	 "permissions": [
	    "tabs", "http://*/*", "https://*/*"
	  ],
  "content_scripts": [
    { "matches": ["http://*/*"], "js": ["jquery.js", "movie_content_script.js"] }
  ],
  "page_action": {
      "default_name": "Movie Tracker Extension",
      "default_icon": "movies.png",
      "popup": "movie_selector.html"
  }
}